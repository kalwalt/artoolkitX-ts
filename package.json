  "scripts": {
    "dev-ts": "webpack --mode development --progress --watch",
    "docs": "typedoc",
    "build": "node tools/makem.js; echo Built at `date`",
    "build-cv": "cmake -S emscripten/artoolkitX_em_2d/Source/depends/emscripten/opencv-3.4.1 -B emscripten/artoolkitX_em_2d/Source/depends/emscripten/opencv-3.4.1/build_opencv-em ",
