    "build-w-docker": "docker exec emscripten-artoolkitx-ts npm run build",
    "build-ts": "webpack --mode production",
    "setup-docker": "docker run -dit --name emscripten-artoolkitx-ts -v $(pwd):/src emscripten/emsdk:2.0.20 bash"
